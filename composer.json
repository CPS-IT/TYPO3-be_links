{
    "name": "cpsit/be-links",
    "description": "Add page or web links as backend modules",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "Backend",
        "Module"
    ],
    "homepage": "https://github.com/CPS-IT/TYPO3-be_links",
    "license": [
        "GPL-2.0+"
    ],
    "authors": [
        {
            "name": "Nicole Cordes | CPS-IT GmbH",
            "email": "cordes@cps-it.de",
            "role": "Developer",
            "homepage": "http://cps-it.de"
        }
    ],
    "require": {
        "typo3/cms-backend": "^6.2 || ^7.6",
        "typo3/cms-core": "^6.2 || ^7.6",
        "typo3/cms-lang": "^6.2 || ^7.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "nimut/testing-framework": "1.x-dev"
    },
    "suggest": {
        "friendsofphp/php-cs-fixer": "Tool to automatically fix PHP coding standards issues"
    },
    "autoload": {
        "psr-4": {
            "CPSIT\\BeLinks\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CPSIT\\BeLinks\\Tests\\": "Tests/",
            "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/"
        }
    },
    "config": {
        "bin-dir": ".Build/bin",
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
